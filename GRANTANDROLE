CREATE ROLE STAFF
COMMIT;
CREATE ROLE ADMINISTRATOR
COMMIT;

GRANT ALL PRIVILEGES TO ADMINISTRATOR
COMMIT;

GRANT SELECT, INSERT, UPDATE, DELETE ON ANIMALS TO STAFF
COMMIT;

GRANT ADMINISTRATOR TO staff_table.staff_email LIKE 'alikhan@gmail.com'

GRANT STAFF TO STAFF_TABLE

CREATE USER ALIKHAN_ADMIN
IDENTIFIED BY adminonly

GRANT ADMINISTRATOR TO ALIKHAN_ADMIN


CREATE USER STAFF_MEMBER
IDENTIFIED BY staffy
commit;

grant staff to staff_member
commit;

grant select, insert, update, delete on staff_table to staff_member
commit;

